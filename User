import java.io.*;
import java.util.*;
public class User 
{
  String password;
  String username;
  int userStatus;
// 1 = guest user 
// 2 = normal user
// 3 = moderator
public User() 
{
    password="";
    username="";
}
  
public User(String u, String p) 
{
	username=u;
    password=p;
    userStatus = 2;
}

public User(int i) {
	username = "Guest";
	password = "";
	userStatus = 1;
}

public String getUsername() 
{
	return username;
}

public String getPassword() 
{
  return password;
}

public int getUserStatus() {
	return userStatus;
}
}
